<Frame name="SuperTreatmentFrame" parent="UIParent" movable="true" frameStrata="DIALOG" enableMouse="true">
		<Size>
			<AbsDimension x="75" y="100" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER">
				<Offset x="0" y="-55" />
			</Anchor>
		</Anchors>
		<Backdrop name="$parentBackdrop" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="3" right="3" top="3" bottom="3" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="12" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="12" />
			</EdgeSize>
		</Backdrop>
		<Frames>
			
			
			
		
		</Frames>
		
		
		
		
	<Scripts>
				
		<OnEvent>
			SuperTreatment_OnEvent(event);
		</OnEvent>
		
		<OnUpdate>
			SuperTreatment_OnUpdate(arg1);
		</OnUpdate>
		
		<OnMouseDown>SuperTreatment_OnMouseDown();
		</OnMouseDown>
		
		<OnMouseUp>SuperTreatment_OnMouseUp();
		</OnMouseUp>
		
	</Scripts>
</Frame>
	
