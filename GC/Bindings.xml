<Bindings>
	<Binding name="RUNONCE" description="" header="MAGICCAST" runOnUp="true">
		MagicCastAddon.Environment:RunScheme();
	</Binding>
	<Binding name="RUNSTART" description="" header="MAGICCAST" runOnUp="true">
		if ( keystate == "down" ) then
			MagicCastAddon.Environment:Start(nil,true);
		else
			MagicCastAddon.Environment:Stop(true);
		end
	</Binding>
	<Binding name="START" description="" header="MAGICCAST">
		MagicCastAddon.Environment:Start();
	</Binding>
	<Binding name="STOP" description="" header="MAGICCAST">
		MagicCastAddon.Environment:Stop();
	</Binding>
</Bindings>
